  <story>
  
    <para style="terp_default_8">[[ repeatIn(objects,'o') ]]</para>
    <para style="terp_default_8">[[ setLang(o.prep_id.partner_id.lang) ]]</para>
    <!-- Document header-->
    <blockTable colWidths="265.0,265.0" style="Table1">
      <tr>
        <td><para style="terp_header">No. IFU: [[ o.company_id.ifu_number ]]</para></td>
        <td><para style="terp_header">No. INSAE: [[ o.company_id.insae_number ]]</para></td>
      </tr>
    </blockTable>
    <para style="terp_default_10">
      <font color="white"> </font>
    </para>
    <para style="terp_default_Centre_10">Déclaration du mois de [[ reformat(formatLang(o.date_to, date=True), '%B %Y') ]]</para>
    <para style="terp_default_10">
      <font color="white"> </font>
    </para>
    <para style="terp_default_10"><b>I - IDENTIFICATION</b></para>
    <blockTable colWidths="265.0,265.0" style="Table8">
      <tr>
        <td><para style="terp_default_9">Nom et Prénom ou Raison Sociale</para></td>
        <td><para style="terp_default_9">[[ o.company_id.name ]]</para></td>
      </tr>
    </blockTable>
    <blockTable colWidths="265.0,265.0" style="Table8">
      <tr>
        <td><para style="terp_default_9">Autre Dénomination</para></td>
        <td><para style="terp_default_9">[[ o.company_id.other_name ]]</para></td>
      </tr>
    </blockTable>
    <blockTable colWidths="265.0,265.0" style="Table8">
      <tr>
        <td><para style="terp_default_9">Activité</para></td>
        <td><para style="terp_default_9">[[ o.company_id.activity ]]</para></td>
      </tr>
    </blockTable>
    <blockTable colWidths="265.0,265.0" style="Table8">
      <tr>
        <td><para style="terp_default_9">Adresse (siège)</para></td>
        <td><para style="terp_default_9">[[ o.company_id.street ]] [[ o.company_id.street2 ]] [[ o.company_id.zip ]] [[ o.company_id.city ]]</para></td>
      </tr>
    </blockTable>
    <blockTable colWidths="265.0,265.0" style="Table8">
      <tr>
        <td><para style="terp_default_9">Tel</para></td>
        <td><para style="terp_default_9">[[ o.company_id.phone ]]</para></td>
      </tr>
    </blockTable>
    <blockTable colWidths="265.0,265.0" style="Table8">
      <tr>
        <td><para style="terp_default_9">Fax</para></td>
        <td><para style="terp_default_9">[[ o.company_id.fax ]]</para></td>
      </tr>
    </blockTable>
    <blockTable colWidths="265.0,265.0" style="Table8">
      <tr>
        <td><para style="terp_default_9">Email</para></td>
        <td><para style="terp_default_9">[[ o.company_id.email ]]</para></td>
      </tr>
    </blockTable>
    <para style="terp_default_10">
      <font color="white"> </font>
    </para>
    <para style="terp_default_10"><b>II - LIQUIDATION DES DROITS</b></para>
    <blockTable colWidths="335.0,200.0" style="Table_Invoice_General_Header">
      <tr>
        <td><para style="terp_default_9">1. Nombre de Salariés</para></td>
        <td><para style="terp_default_Right_9">[[ len(subset(o.line_ids, 'AIBREG')) ]]</para></td>
      </tr>
      <tr>
        <td><para style="terp_default_9">2. Montant brut des Salaires</para></td>
        <td><para style="terp_default_Right_9">[[ formatLang(sum_line(subset(o.line_ids, 'AIBREG'),'gross'), dp='Account', currency_obj=o.currency_id) ]]</para></td>
      </tr>
      <tr>
        <td><para style="terp_default_9">3. Montant des prélèvements</para></td>
        <td><para style="terp_default_Right_9">[[ formatLang(sum_line(subset(o.line_ids, 'AIBREG'),'total'), dp='Account', currency_obj=o.currency_id) ]]</para></td>
      </tr>
    </blockTable>
    <blockTable colWidths="335.0,200.0" style="Table_Invoice_General_Header">
      <tr>
        <td><para style="terp_default_9">4. Net à Payer</para></td>
        <td><para style="terp_default_Right_9"><b>[[ formatLang(sum_line(o.line_ids,'total'), dp='Account', currency_obj=o.currency_id) ]]</b></para></td>
      </tr>
    </blockTable>
    <para style="terp_default_8">
      <font color="white"> </font>
    </para>
    <para style="terp_default_Centre_10">Arrêté la présente déclaration à la somme de <b>[[o.voucher_id.amount_in_word ]]</b>, soit <b>[[ formatLang(sum_line(o.line_ids,'total'), dp='Account', currency_obj=o.currency_id) ]]</b></para>
    <para style="terp_default_10">
      <font color="white"> </font>
    </para>
    <para style="terp_default_10"><b>III - PAIEMENT</b></para>

    <blockTable colWidths="235.0,300.0" style="Table1">
      <tr>
        <td>
          <para style="terp_default_9">Chèque No: [[ o.voucher_id.reference ]]</para>
          <para style="terp_default_9">Banque: [[ o.bank_id.bank.name ]]</para>
          <para style="terp_default_9">Date d'émission: [[ o.voucher_id.date ]]</para>
          <para style="terp_default_9">Payé le:</para>
        </td>
        <td>
          <blockTable colWidths="300" style="Table_General_Detail_Content">
            <tr>
              <td><para style="terp_default_9"><b>Cadre Réservé à l'administration</b></para></td>
            </tr>
          </blockTable>
          <blockTable colWidths="300.0" style="Table_General_Detail_Content">
            <tr>
              <td>
                <para style="terp_default_9">No. de quittance</para>
                <para style="terp_default_10"><font color="white"> </font></para>
              </td>
            </tr>
          </blockTable>
          <blockTable colWidths="100,100,100" style="Table_General_Detail_Content">
            <tr>
              <td><para style="terp_default_9">Pénalité</para></td>
              <td><para style="terp_default_9">Motif</para></td>
              <td><para style="terp_default_9">Montant</para></td>
            </tr>
          </blockTable>
          <blockTable colWidths="100,100,100" style="Table_General_Detail_Content">
            <tr>
              <td><para style="terp_default_9">AIB</para></td>
              <td><para style="terp_default_9"></para></td>
              <td><para style="terp_default_9"></para></td>
            </tr>
          </blockTable>
          <blockTable colWidths="300" style="Table_General_Detail_Content">
            <tr>
              <td>
                <para style="terp_default_9">Montant total</para>
                <para style="terp_default_10"><font color="white"> </font></para>
              </td>
            </tr>
          </blockTable>

        </td>
      </tr>
    </blockTable>

    <para style="terp_default_10">
      <font color="white"> </font>
    </para>
